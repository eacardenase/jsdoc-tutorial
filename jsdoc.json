{
    "plugins": ["jsdoc-http-plugin"],
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": "false",
        "monospaceLinks": "false"
    },
    "opts": {
        "recurse": "true",
        "destination": "./doc",
        "template": "node_modules/docdash",
        "tutorials": "./tutorials",
        "readme": "./Readme.md"
    }
}
